{
  "name": "tween-titan",
  "version": "0.0.3",
  "description": "A front-end library that makes it easy to transition an element on your website from one style to another when the user scrolls.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "dev": "yarn start && webpack serve --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pub": "yarn build && yarn publish"
  },
  "author": "Andrew McGill",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "webpack": "^5.12.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "d3-interpolate": "^3.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "lodash": "^4.17.21"
  }
}
